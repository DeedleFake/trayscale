{{- $id := or .ID "parent" -}}
{{- $used := newValueSet -}}

{{range .Properties -}}
	{{$id}}.SetObjectProperty({{.Name | printf "%q"}}, {{if .Track}}{{$used.Add .Value}}{{else}}{{.Value}}{{end}})
{{end -}}
{{range $i, $c := .Children -}}
	{{- $cid := or .Object.ID (printf "%v%v" $id $i) -}}
	{{if not ($used.Has $cid) -}}
		{{$id}}.{{$.Type.AddChild .Type $cid}}
	{{end -}}
{{end}}
